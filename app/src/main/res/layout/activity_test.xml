<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Test">
    <Button
        android:id="@+id/go_setting_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="go_setting_time"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/go_user_information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="go_user_information"
        app:layout_constraintTop_toBottomOf="@id/go_setting_time"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <Button
        android:id="@+id/push"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PushNotification"
        app:layout_constraintTop_toBottomOf="@id/go_user_information"
        app:layout_constraintLeft_toLeftOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>